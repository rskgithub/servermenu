	<table class="table table-striped table-media table-condensed" style="table-layout:fixed;">
	    	<tbody>
		    	{% for File in FileList %}
		    	<tr>
		    	    <td style="text-align: left; word-wrap: break-word; width:100%">
                        <span class="text-muted small">{{ File.modified }} &nbsp; {{ File.size }}</span><br />
		    	    	<a href="http://servermenu.net{{ File.path }}/{{ File.filename }}">{{ File.filename }}</a>
		    	    </td>
		    	</tr>
		    	{% endfor %}
			<tbody>
		</table>